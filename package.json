  "scripts": {
    "dev": "tsc --watch",
    "exportStyle": "npx tailwindcss --postcss -c ./tailwind.config.cjs -i src/style.css -o dist/build.css --minify",
    "exportNextStyle": "npx tailwindcss --postcss -c ./tailwind.config.cjs -i src/style.css -o dist/build.css --minify",
    "prebuild": "rm -rf build",
    "build": "bump && tsc",
    "buildNoBump": "tsc",
    "postbuild": "sh ./src/scripts/postbuild.sh && npm run exportStyle && npm run exportNextStyle",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "prettier": "prettier --check \"./**/**/*.{json,js,ts,tsx,scss}\"",
    "prettier-format": "prettier --config .prettierrc \"./**/**/*.{json,js,ts,tsx,scss,md}\" --write",
    "export": "tsc && next build && next export -o ipfs"
  },
